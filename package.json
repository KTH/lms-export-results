{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-export-results",
  "description": "Export results (betygsunderlag) from canvas to something that can be used for Ladok import (first attempt)",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test:lint": "eslint .",
    "test:format": "prettier --check .",
    "test:unit": "tape \"test/specs/*.test.js\" | tap-spec",
    "test": "npm run test:unit && npm run test:lint && npm run test:format",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "nodemon app.js",
    "format": "prettier --write .",
    "debug": "NODE_ENV=development node --nolazy --inspect-brk=9229 app.js | bunyan -o short"
  },
  "dependencies": {
    "@kth/reqvars": "^2.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "got": "^9.6.0",
    "kth-canvas-api": "^0.15.7",
    "kth-node-server": "^3.1.2",
    "kth-style": "^3.2.18",
    "ldapts": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "popper.js": "^1.16.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^1.0.13",
    "eslint": "^6.8.0",
    "fs-extra": "^7.0.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.3.2",
    "proxyquire": "^2.1.3",
    "randomstring": "^1.1.5",
    "rewire": "^4.0.1",
    "should": "^13.2.3",
    "sinon": "^7.4.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2"
  }
}
