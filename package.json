{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-export-results",
  "description": "Export results (betygsunderlag) from canvas to something that can be used for Ladok import (first attempt)",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "tape \"test/specs/*.test.js\" | tap-spec",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "nodemon app.js",
    "test:docker-unit": "npm install --no-optional; npm test",
    "debug": "NODE_ENV=development node --nolazy --inspect-brk=9229 app.js | bunyan -o short"
  },
  "standard": {
    "ignore": [
      "**/test/"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "got": "^9.6.0",
    "kth-canvas-api": "^0.15.7",
    "kth-node-server": "^3.1.1",
    "kth-style": "^3.2.18",
    "ldapjs": "^1.0.2",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "fs-extra": "^7.0.1",
    "nodemon": "^1.19.2",
    "proxyquire": "^2.1.3",
    "randomstring": "^1.1.5",
    "rewire": "^4.0.1",
    "should": "^13.2.3",
    "sinon": "^7.4.2",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "engines": {
    "node": "8.6.0"
  }
}
