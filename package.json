{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-export-results",
  "description": "Export results (betygsunderlag) from canvas to something that can be used for Ladok import (first attempt)",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "tape \"test/specs/*.test.js\" | tap-spec",
    "test:integration": "tape \"test/integration/*.test.js\" | tap-spec",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "nodemon app.js",
    "install-and-test": "npm install && npm test --verbose"
  },
  "standard": {
    "ignore": [
      "**/cypress/",
      "**/test/"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bunyan": "^1.5.1",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "kth-canvas-api": "^0.15.2",
    "kth-canvas-utilities": "github:kth/kth-canvas-utilities",
    "kth-node-access-log": "github:KTH/kth-node-access-log#v1.0.0",
    "kth-node-api-common": "^0.1.3",
    "kth-node-api-key-strategy": "^1.0.5",
    "kth-node-configuration": "^1.6.3",
    "kth-node-express-routing": "^0.3.1",
    "kth-node-response": "KTH/kth-node-response.git#v1.0.0",
    "kth-node-server": "^3.1.1",
    "kth-style": "^2.0.0-beta.2",
    "ldapjs": "^1.0.1",
    "moment": "^2.20.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "safe-utils": "0.1.0"
  },
  "devDependencies": {
    "cypress": "^1.4.1",
    "fs-extra": "^5.0.0",
    "nodemon": "^1.14.7",
    "proxyquire": "^1.8.0",
    "randomstring": "^1.1.5",
    "rewire": "^2.5.2",
    "rewire-global": "^1.0.11",
    "rimraf": "^2.6.2",
    "rimraf-promise": "^2.0.0",
    "selenium-webdriver": "^3.6.0",
    "should": "^13.2.0",
    "sinon": "^4.1.3",
    "standard": "^10.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "engines": {
    "node": "8.6.0"
  }
}
